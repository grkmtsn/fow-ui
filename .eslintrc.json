{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "extends": [
        "airbnb",
        "airbnb/hooks"
    ],
    "env": {
        "browser": true
    },
    "globals": {
        "React": true
    },
    "rules": {
        "linebreak-style": 0,
        "react/jsx-props-no-spreading": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-named-as-default": 0,
        "import/named": 0,
        "max-len": 0,
        "react/forbid-prop-types": [
            1,
            {
                "forbid": [
                    "any"
                ]
            }
        ]
    }
}