{
  "name": "fow-ui",
  "version": "0.4.0",
  "description": "Fow UI Library wrapped around Antd",
  "keywords": [
    "react",
    "antd",
    "webpack",
    "styleguidist",
    "fow"
  ],
  "main": "dist/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-cli --progress --colors",
    "build": "cross-env NODE_ENV=production webpack-cli --progress --colors",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "postbuild": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "prepublishOnly": "npm run build"
  },
  "author": "grkkmm1@gmail.com",
  "license": "ISC",
  "repository": "github:grkmtsn/fow-ui",
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "files": [
    "/dist"
  ],
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "antd": "^4.9.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "antd": "^4.9.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "customize-cra": "^1.0.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "react-styleguidist": "^11.1.5",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.4.0"
  }
}
